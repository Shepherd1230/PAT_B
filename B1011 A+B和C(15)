#include<iostream>
#include<string.h>

using namespace std;

int main()
{
	int n;
	cin>>n;
	
	long long nums[10][3];
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<3;j++)
		{
			cin>>nums[i][j];
		}
	}
	for(int i=0;i<n;i++)
	{
		if(nums[i][0]+nums[i][1]>nums[i][2])
		{
			cout<<"Case #"<<i+1<<": true"<<endl;
		}
		else
		{
			cout<<"Case #"<<i+1<<": false"<<endl;
		}
	}
		
	return 0;
}
