#include<iostream>
#include<vector>
#include<string>
#include<stdio.h>
using namespace std;

void getmax(vector<double>list,int &i)
{
	int max=0;//i returns the root of the max
	
	for(int j=0;j<3;j++)
	{
		if(list.at(j)>list.at(max))
		{
			max=j;
		}
	}
	
	i=max;
}

int main()
{
	vector<vector<double> >data;
	for(int i=0;i<3;i++)
	{
		vector<double>temp;
		for(int j=0;j<3;j++)
		{
			double t;
			cin>>t;
			temp.push_back(t);
		}
		data.push_back(temp);
	}
	
	
	
	vector<int> num(3,0);
	for(int i=0;i<3;i++)
	{
		getmax(data.at(i),num.at(i));
		
		if(num.at(i)==0)
		{
			cout<<"W ";
		}
		else if(num.at(i)==1)
		{
			cout<<"T ";
		}
		else
		{
			cout<<"L ";
		}
	}
	
	double temp;
	temp=(data.at(0).at(num.at(0))*data.at(1).at(num.at(1))*data.at(2).at(num.at(2))*0.65-1)*2;
	
	
	
	printf("%.2f\n",temp);

	
	return 0;
}
